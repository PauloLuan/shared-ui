{
  "name": "shared-ui",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauloluan-ui/shared-ui.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "pkg:run": "manypkg run",
    "pkg:fix": "manypkg fix",
    "pkg:check": "manypkg check",
    "dedup": "yarn-deduplicate --strategy highest yarn.lock",
    "postinstall": "yarn dedup && lerna exec sort-package-json --parallel && yarn pkg:check",
    "clean:full": "lerna clean --yes && rimraf node_modules",
    "clean:dist": "yarn lerna exec rimraf dist",
    "clean:storybook": "rimraf node_modules/.cache/storybook",
    "clean:motherfucker": "git clean -xfd && yarn",
    "build": "yarn clean:dist && yarn lerna run build --no-private --stream",
    "prestorybook:start": "yarn clean:storybook && yarn workspace @shared-ui/bootstrap build:sass",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build && build-storybook && sb extract",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --maxWorkers=25% --watch",
    "test:ci": "jest --runInBand",
    "release": "yarn build && lerna version --force-publish",
    "release:minor": "yarn release minor",
    "release:patch": "yarn release patch"
  },
  "resolutions": {
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "dependencies": {
    "@babel/cli": "~7.15.7",
    "@babel/core": "~7.15.0",
    "@babel/preset-env": "~7.15.0",
    "@babel/preset-react": "~7.14.5",
    "@babel/preset-typescript": "~7.15.0",
    "@manypkg/cli": "~0.18.0",
    "@storybook/addon-actions": "~6.3.8",
    "@storybook/addon-essentials": "~6.3.8",
    "@storybook/addon-links": "~6.3.8",
    "@storybook/addon-storysource": "~6.3.8",
    "@storybook/addons": "~6.3.8",
    "@storybook/node-logger": "~6.3.8",
    "@storybook/react": "~6.3.8",
    "@storybook/theming": "~6.3.8",
    "@testing-library/jest-dom": "~5.14.1",
    "@testing-library/react": "~12.0.0",
    "@testing-library/react-hooks": "~7.0.1",
    "@testing-library/user-event": "~13.2.1",
    "@types/jest": "~27.0.1",
    "@types/lodash": "4.14.171",
    "@types/node": "~16.3.1",
    "@types/react": "~17.0.19",
    "@types/react-datepicker": "~4.1.3",
    "@types/react-dom": "~17.0.9",
    "@types/react-router-dom": "~5.1.8",
    "@types/styled-components": "~5.1.11",
    "@types/styled-system": "~5.1.12",
    "@types/testing-library__jest-dom": "~5.14.1",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.2.2",
    "babel-plugin-annotate-pure-calls": "~0.4.0",
    "babel-plugin-dev-expression": "~0.2.2",
    "babel-plugin-macros": "~3.1.0",
    "bootstrap": "4.6.0",
    "concurrently": "~6.2.0",
    "core-js": "~3.15.2",
    "cross-env": "~7.0.3",
    "css-loader": "~6.2.0",
    "dotenv-cli": "~4.0.0",
    "eslint": "~7.30.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-testing-library": "~3.9.2",
    "esm": "3.2.25",
    "identity-obj-proxy": "~3.0.0",
    "jest": "~27.0.6",
    "jest-styled-components": "~7.0.5",
    "lerna": "~4.0.0",
    "lint-staged": "~11.0.1",
    "miragejs": "0.1.42",
    "nodemon": "~2.0.13",
    "npm-check-updates": "~11.8.2",
    "pkg-dir": "~4.2.0",
    "plop": "~2.7.4",
    "prettier": "2.3.2",
    "react": "~17.0.2",
    "react-bootstrap": "~1.6.1",
    "react-dom": "~17.0.2",
    "react-router-dom": "~5.2.0",
    "rimraf": "~3.0.2",
    "sass": "1.32.13",
    "sort-package-json": "~1.50.0",
    "styled-components": "~5.3.1",
    "styled-media-query": "~2.1.2",
