{
  "name": "pauloluan-ui",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "bootstrap": "lerna bootstrap && yarn build",
    "build": "lerna run --stream build",
    "predev": "yarn bootstrap",
    "dev": "lerna run dev --stream --parallel",
    "prestart": "yarn bootstrap",
    "start": "lerna run start --stream --parallel",
    "format": "eslint --fix packages --ext .js,.jsx,.ts,.tsx",
    "pretest": "yarn bootstrap",
    "test": "lerna run --parallel test",
    "test:watch": "lerna run --parallel test:watch",
    "bump": "yarn npm-check-updates --reject 'bootstrap react-bootstrap' -u",
    "deps:upgrade": "yarn sort-package-json && yarn bump && yarn dedup && yarn",
    "prestorybook": "rimraf node_modules/.cache/storybook",
    "shared-ui:start": "lerna run dev --scope @pauloluan/shared-ui",
    "storybook:start": "start-storybook -p 6006 -s .storybook/public && yarn bootstrap",
    "storybook": "concurrently \"yarn storybook:start\"",
    "dedup": "yarn-deduplicate --strategy highest yarn.lock",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "clean:dist": "yarn lerna exec -- rimraf dist",
    "clean:motherfucker": "git clean -xfd && yarn"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "bootstrap": "4.6.0",
    "core-js": "^3.15.2",
    "framer-motion": "4.1.17",
    "miragejs": "0.1.42",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-datepicker": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0",
    "styled-icons": "^10.35.0",
    "styled-media-query": "^2.1.2",
    "sweetalert2": "^11.0.18",
    "sweetalert2-react-content": "^4.1.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "7.14.8",
